    "contextModule": "server/appContext.js",
    "routes": [{"match": "/jsm/client.js",
		"matchOn": "absolute",
		"resourceClass": "internal",
		"internalResourcePath": "/client/JSM.js",
		"action": "serve",
		"success": 200,
		"failure": 500},
	       {"match": "/favicon.ico",
		"matchOn": "absolute",
		"resourceClass": "application",
		"action": "serve",
		"success": 200,
		"failure": 404},
	       {"match": "^server/.*$",
		"matchOn": "relative",
		"resourceClass": "document",
		"action": "reject",
		"reject": 403},
	       {"match": ".*",
		"matchOn": "absolute",
		"resourceClass": "document",
		"action": "serve",
		"success": 200,
		"failure": 404}]
}